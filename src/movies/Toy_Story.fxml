<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane id="AnchorPane" nodeOrientation="LEFT_TO_RIGHT"
	prefHeight="487.0" prefWidth="673.0"
	xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1"
	fx:controller="movies.MovieController">
	<children>
		<ImageView fitHeight="465.0" fitWidth="170.0" layoutX="8.0"
			layoutY="14.0">
			<image>
				<Image url="@../../../../../../../Área%20de%20Trabalho/22.png" />
			</image>
		</ImageView>
		<Text layoutX="27.0" layoutY="40.0" strokeType="OUTSIDE"
			strokeWidth="0.0" text="Bem-vindo!" textAlignment="RIGHT"
			underline="true">
			<font>
				<Font name="System Bold" size="24.0" />
			</font>
		</Text>
		<ButtonBar blendMode="SRC_ATOP" layoutX="-8.0"
			layoutY="448.0" prefHeight="17.0" prefWidth="158.0">
			<buttons>
				<Button fx:id="btHome" mnemonicParsing="false"
					onAction="#onBtHomeAction" text="Início">
					<font>
						<Font name="System Bold" size="12.0" />
					</font>
				</Button>
				<Button fx:id="btExit" mnemonicParsing="false"
					onAction="#onBtExitAction" text="Sair">
					<font>
						<Font name="System Bold" size="12.0" />
					</font>
				</Button>
			</buttons>
		</ButtonBar>
		<AnchorPane layoutX="190.0" layoutY="14.0"
			prefHeight="465.0" prefWidth="469.0" />
	</children>
</AnchorPane>
